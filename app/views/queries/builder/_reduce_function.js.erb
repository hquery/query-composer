function reduce(key, values) {
  
  if (key['type'] == 'magic') {
    var sum = 0;
    
    var result = {rereduce: true}
    
    for(var i in values) {
      sum += values[i]['values'][0]['value'];
    }
    
    result['values'] = [{value:sum}];
    
    return result;
